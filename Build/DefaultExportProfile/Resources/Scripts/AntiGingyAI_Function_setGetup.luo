






function AntiGingyAI.setGetup()





local hasAnimation=object.hasController(this.hEnemy(),object.kControllerTypeAnimation)


if(hasAnimation==true)then

animation.setCurrentClip(this.hEnemy(),0,hashtable.get(this.htAnimations(),"idle"))





animation.setPlaybackKeyFrameBegin(this.hEnemy(),0,hashtable.get(this.htAnimations(),"idle_begin"))


animation.setPlaybackKeyFrameEnd(this.hEnemy(),0,hashtable.get(this.htAnimations(),"idle_end"))

end



end
