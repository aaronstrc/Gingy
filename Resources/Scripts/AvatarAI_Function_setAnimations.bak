--------------------------------------------------------------------------------
function AvatarAI.setAnimations ( )
--------------------------------------------------------------------------------
	
    --adds the animations to a hash table to be called upon
    
    hashtable.add ( this.animations ( ), "run", 1)
    hashtable.add ( this.animations ( ), "jump", 2)
    hashtable.add ( this.animations ( ), "idle", 3)
    hashtable.add ( this.animations ( ), "punch", 4)
    
    
	
--------------------------------------------------------------------------------
end
--------------------------------------------------------------------------------
